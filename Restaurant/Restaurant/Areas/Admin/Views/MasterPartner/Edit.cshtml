@model Restaurant.ViewModels.MasterPartnerViewModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<div class="pagetitle">
      
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a asp-controller="MasterPartner" asp-action="index">Master Partner</a></li>
           
          <li class="breadcrumb-item active">Edit</li>
          
        </ol>
      </nav>
    </div>
<section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
        <form asp-action="Edit" enctype="multipart/form-data" class="row mb-3">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MasterPartnerId" />
            <div class="row mb-1">
                <label asp-for="MasterPartnerName" class="control-label"></label>
                <input asp-for="MasterPartnerName" class="form-control" />
                <span asp-validation-for="MasterPartnerName" class="text-danger"></span>
            </div>
            
            <div class="row mb-1">
                <label asp-for="MasterPartnerWebsiteUrl" class="control-label"></label>
                <input asp-for="MasterPartnerWebsiteUrl" class="form-control" />
                <span asp-validation-for="MasterPartnerWebsiteUrl" class="text-danger"></span>
            </div>
            <div class="row mb-1">
                <label asp-for="LogoImage" class="control-label"></label>
                <input asp-for="LogoImage" class="form-control" />
                <span asp-validation-for="LogoImage" class="text-danger"></span>
            </div>
            <div>
                <input asp-for="MasterPartnerLogoImageUrl" type="hidden"/>

            </div>
            
            <div style=" direction: rtl"  class="form-group">
                <input type="submit" value="Save" class="btn btn-primary rounded-pill" />
                <button asp-action="Index"  class="btn btn-primary rounded-pill" >Back to List</button>
            </div>
        </form>
    </div>
          </div>

        </div>
      </div>
    </section>



