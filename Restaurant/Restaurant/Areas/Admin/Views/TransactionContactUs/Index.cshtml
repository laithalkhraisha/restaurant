@model IEnumerable<RESTAURANT.Models.TransactionContactUs>

@{
    ViewData["Title"] = "Index";
}

<div class="pagetitle">
    <h1>Transaction ContactUs</h1>
    <nav>
        <ol class="breadcrumb">
